rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /user-outputs/{uid}/{allPaths=**} {
      allow read: if request.auth != null && request.auth.uid == uid;
      allow write: if request.auth != null && request.auth.uid == uid;
    }
    match /public/{allPaths=**} { allow read; }
  }
}
